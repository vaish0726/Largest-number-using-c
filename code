#include <stdio.h>

int main()
{
    int a;
    int b;
    int c;
    
    printf("Enter 3 numbers: \n");
    scanf("%d %d %d",&a,&b,&c);
    
    if(a>b && a>c){
        printf("Largest number is : %d",a);
    }
    else if(b>a && b>c){
        printf("Largest number is : %d",b);
    }
    else{
        printf("Largest number is %d",c);
    }
}
